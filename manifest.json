{
  "name": "Auto Clicker",
  "description": "Tesing Description",
  "author": "Omer Bhatti",
  "version": "0.0.1",
  "manifest_version": 2,

  "browser_action": {
    "default_popup":"popup.html",
    "default_icon":"/assets/images/icon.png",
    "default_title":"Auto clicker"
  },

  "icons":{
    "300":"/assets/images/icon.png"
  },
  
  "permissions":["tabs"],

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
